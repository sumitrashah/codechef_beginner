#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
const long long INF = 1e18 + 42;
int main() {
ll t;
	ignore=scanf("%d",&t);
	while(t--)
	{
	       ll n,m;
	      ignore=scanf("%lld %lld",&n,&m);
	       ll x, mux=1;
	       
	       while(n--)
	       {
	            
	           ignore=scanf("%lld",&x);
	             if(x>INF/mux)
	             mux=INF;
	             else
	             mux*=x;
	       }
	       while(m--)
	       {
	              
	             ignore=scanf("%lld",&x);
	              printf("%lld ",x/mux);
	       }
	    printf("\n");
	}
	  
	return 0;
}

